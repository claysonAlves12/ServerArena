{
  "version": 2,
  "functions": {
    "api": {
      "runtime": "node@14",
      "handler": "server.js"
    },
    "websocket": {
      "runtime": "node@14",
      "allowConfig": true,
      "dest": "websocket.js"
    }
  },
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
