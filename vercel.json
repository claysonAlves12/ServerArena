{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "/public/(.*)",
      "use": "@vercel/static"
    },
    {
      "src": "/css/(.*)",
      "use": "@vercel/static"
    }
  ],
"routes": [
    {
      "src": "/.*",
      "dest": "/server.js"
    }
  ]
}
