{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/nod"
    },
    {
      "src": "/public/(.*)",
      "use": "@vercel/static"
    },
    {
      "src": "/styles/(.*)",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "/server.js"
    }
  ]
}
