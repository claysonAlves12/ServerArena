{
  "version": 2,
  "functions": {
    "api": "server.js",  
    "websocket": {
      "handler": "websocket.js",  
      "allowConfig": true
    }
  },
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
